<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    
    <!-- Plugin Informations -->
    <name>menus</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>2.0.1</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>menus.plugin.description</description>
    <provider>menus.plugin.provider</provider>
    <provider-url>http://fr.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/menus/menus.png</icon-url>
    <copyright>Copyright 2001-2009 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>2.3.0</min-core-version>
		<max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>
    
    <!-- Page Include Service -->
    <page-include-services>
        <page-include-service>
            <page-include-service-id>tree_menu_page</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.menus.web.TreeMenuPageInclude</page-include-service-class>
        </page-include-service>
        <page-include-service>
            <page-include-service-id>xpage_menu</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.menus.web.XPageMenuInclude</page-include-service-class>
        </page-include-service>	
        <page-include-service>
            <page-include-service-id>main_tree_menu</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.menus.web.MainTreeMenuInclude</page-include-service-class>
        </page-include-service>
        <page-include-service>
            <page-include-service-id>main_tree_menu_all_pages</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.menus.web.MainTreeMenuIncludeAllPages</page-include-service-class>
        </page-include-service>
        <page-include-service>
            <page-include-service-id>accessibility_anchor_menu</page-include-service-id>
            <page-include-service-class>fr.paris.lutece.plugins.menus.web.AccessibilityAnchorMenuInclude</page-include-service-class>
        </page-include-service>
    </page-include-services>	
	
    <!-- Administration interface parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>MENUS_MANAGEMENT</feature-id>
            <feature-title>menus.adminFeature.menus_management.name</feature-title>
            <feature-description>menus.adminFeature.menus_management.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/menus/ManageMenus.jsp</feature-url>
        </admin-feature>
    </admin-features>
 </plug-in>